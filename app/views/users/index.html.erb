<a href="/users/new">New</a>

<%= form_with model: @user do |form| %>
  <%= form.collection_select(:id, @users, :id, :username) %>
  <%= form.submit "Edit" %>
<% end %>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.username %>
      <%= link_to "Edit", edit_user_path(user) %>
    </li>
  <% end %>
</ul> 
